pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
            stage('Build') {
            steps {
                echo 'Building'
            }
        }    
            stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }    
            stage('test') {
            steps {
                echo 'Test'
            }
        }   
            stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
